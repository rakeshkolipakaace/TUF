class Solution {
  public:
    double switchCase(int choice, vector<double> &arr) {
        
        switch(choice){
            case 1: return 3.141592653589793*arr[0]*arr[0];
            case 2: return arr[0]*arr[1];
            default: return -1 ;
        }
        // code here
    }
};


